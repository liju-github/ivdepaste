
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}
        
        
model paste {
  id         String   @id @default(uuid())
  title      String
  content    String
  userId     String?
  burn       Boolean @default(false)
  createdAt  DateTime @default(now())
  expiresAt  DateTime?
  visibility String
  language   String
}
